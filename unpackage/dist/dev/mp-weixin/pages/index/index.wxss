page {
  height: 100%;
}
.content .title {
  padding: 2rpx 10rpx;
  background-color: #f7f7f7;
}
.content .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.content .item .icon {
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

