<view class="content list"><scroll-view class="scroll-box" scroll-y="true" scroll-into-view="{{'NAV'+toNum}}" enable-back-to-top="true" scroll-with-animation="true" scroll-top="{{scrollTop}}" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-group"><view class="title" id="{{'NAV'+index}}">{{''+item.Title+''}}</view><block wx:for="{{item.List}}" wx:for-item="info" wx:for-index="secondIndex" wx:key="secondIndex"><view data-event-opts="{{[['tap',[['enterDetail',['$0'],[[['list','',index],['List','',secondIndex,'petBreedId']]]]]]]}}" class="item" bindtap="__e"><image class="icon" src="{{info.icon}}" mode lazy-load="true"></image><text>{{info.name}}</text></view></block></view></block></scroll-view><view class="list-shortcut"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-id="{{item.Title}}" data-index="{{index}}" data-event-opts="{{[['tap',[['scrollToview',['$event']]]]]}}" bindtap="__e">{{item.Title}}</text></block></view><view class="{{['list-fixed',fixedTitle==''?'hide':'']}}" style="{{'transform:'+('translate3d(0,'+fixedTop+'px,0)')+';'}}"><view class="fixed-title">{{''+fixedTitle+''}}</view></view></view>